msgid "(32000.00)"
msgstr "(32000.00)"

#: ../../content/applications/general/export_import_data.rst:171
msgid "$ 32.000,00"
msgstr "$ 32.000,00"

#: ../../content/applications/general/export_import_data.rst:172
msgid "(32000.00 €)"
msgstr "(32000.00 €)"

